{
  "extends": "google",
  "parser": "babel-eslint",
  globals: {
    document: true,
    XMLHttpRequest: true,
    window: true
  },
  "rules": {
    "max-len": [1, 200, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }],
    "valid-jsdoc": 0,
    "curly": 0,
    "one-var": 0,
    "no-implicit-coercion": 0,
    "consistent-return": 0,
    "no-negated-condition": 0
  }
}
